   10REM > m7chars
   20LIBRARY "../SAA505Xlib"
   30MODE 7
   35OFF
   40FOR x% = 0 TO 7
   50FOR y% = 160 TO 255 STEP 8
   60VDUx%+y%,32
   70NEXT y%:PRINT
   80NEXT x%
   90N%=0
  100REPEAT
  105PRINTTAB(0,8)"SAA505";N%
  110PROCsaa505xlibset(N%)
  120WAIT300
  130N%+=1
  140IFN%=8THENN%=0
  150UNTILN%=0
